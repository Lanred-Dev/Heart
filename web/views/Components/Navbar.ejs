<navbar>
    <div class="Topbar_Container">
        <div class="Latest_Announcement Main_Item_Container" id="Latest_Announcement">
            <div class="Content Announcement Cover_Whole Page_Center_Align">
                <div class="Left_Container">
                    <p class="Announcement_Content" id="Latest_Announcement_Content"></p>
                </div>

                <div class="Content">
                    <div class="Buttons">
                        <button id="Latest_Announcement_Close_Button">X</button>
                    </div>
                </div>
            </div>

            <div class="Background Cover_Whole">
                <div class="Content Background"></div>
            </div>
        </div>

        <div class="Navbar Main_Item_Container" id="Navbar">
            <div class="Content Navbar Cover_Whole Page_Center_Align">
                <div class="Left_Container">
                    <img class="Logo" src="http://localhost:3000/Images/Logo.png" draggable="false" alt="Logo">
                    <p class="Title Text_Effects Slight_Glow">Heart</p>
                </div>

                <div class="Content">
                    <ul class="List Responsive">
                        <div class="Links">
                            <a class="Link" href="http://localhost:3000/features/color">Help</a>
                            <a class="Link" href="http://localhost:3000/features/color">Features</a>
                            <a class="Link" href="http://localhost:3000/features/color">Contact</a>
                            <a class="Link" href="http://localhost:3000/features/color">Support</a>
                        </div>

                        <div class="Account_Details">
                            <div id="Navbar_Account_Details" data-user_details=<%= User %>>
                                <img id="Navbar_Account_Details_Icon" src="http://localhost:3000/Images/Logo.png">
                                <p id="Navbar_Account_Details_Username">NA</p>
                            </div>

                            <a class="Meta_Button Black Small_Padding" id="Navbar_Login_Button" onclick="window.open('http://localhost:3000/login', '_blank', 'height=700, width=550, scrollbars=yes');">Login</a>
                        </div>
                    </ul>
                </div>
            </div>

            <div class="Background Cover_Whole">
                <div class="Content Background"></div>
            </div>
        </div>
    </div>
</navbar>

<script type="text/javascript" src="http://localhost:3000/JS/Account_Handler.js"></script>